[[inputs.cpu]]
  percpu = true
  totalcpu = true
  fielddrop = ["time_*"]

[[inputs.disk]]
  ignore_fs = ["tmpfs", "devtmpfs"]

[[inputs.diskio]]
[[inputs.kernel]]
[[inputs.mem]]
[[inputs.processes]]
[[inputs.swap]]
[[inputs.system]]

[[inputs.snmp]]
  agents = ["192.168.7.1"]
  version = 1
  community = "unifi"
  
  [[inputs.snmp.field]]
  name = "uptime"
  oid = "DISMAN-EVENT-MIB::sysUpTimeInstance"

  [[inputs.snmp.field]]
  name = "avrload_1min"
  oid = "UCD-SNMP-MIB::laLoadInt.1"
  conversion = "float(0)"

  [[inputs.snmp.field]]
  name = "avrload_5min"
  oid = "UCD-SNMP-MIB::laLoadInt.2"
  conversion = "float(0)"

  [[inputs.snmp.field]]
  name = "avrload_15min"
  oid = "UCD-SNMP-MIB::laLoadInt.3"
  conversion = "float(0)"

  [[inputs.snmp.field]]
  name = "available_memory"
  oid = "HOST-RESOURCES-MIB::hrMemorySize.0"
  conversion = "int"
